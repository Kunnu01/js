{
  "name": "@ironplans/api",
  "license": "MIT",
  "version": "0.0.32-alpha.0",
  "browser": "./dist/api.esm.js",
  "module": "./dist/api.esm.js",
  "exports": {
    ".": "./dist/api.esm.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ironplans/js.git"
  },
  "scripts": {
    "prepare": "make -s all lint -j8",
    "start": "make serve",
    "lint": "make lint -j8",
    "build": "make all format -j8",
    "format": "make format -j8",
    "clean": "make clean",
    "generate": "../scripts/gen-browser.sh -d ./src",
    "analyze": "size-limit --why"
  },
  "bugs": {
    "url": "https://github.com/ironplans/js/issues"
  },
  "size-limit": [
    {
      "path": "dist/api.esm.js",
      "limit": "10 kB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4"
  }
}
